import java.util.*;
class Solution
{    
 public static void main(String[] args) 
{    
       Scanner sc=new Scanner(System.in);
       int i,m=0,l=0;
       System.out.println("enter array size");
       int n=sc.nextInt();
       int a[]=new int[n];
       System.out.println("enter array elements");
       for(i=0;i<n;i++)
       {
       	a[i]=sc.nextInt();
       }
       for(i=0;i<n;i++)
       {
       	if(a[i]%2==0)
       	l++;
       	else 
       	m++;
       }
       int product=l*m;
       System.out.println("total count :"+product);
       if(product%2==0)
       
       	System.out.println("even");
       
       else
       System.out.println("odd");
     
       
    }    
}
